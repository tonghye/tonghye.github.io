// click tracking function
function sendClickTrack(ClickType,ClickName) {
  s.linkTrackVars="prop52,prop53,prop55,eVar53";  // eVar53 트래킹 추가 2017-06-05
  s.prop52=ClickType;
  s.prop53=ClickType + ":" + ClickName;   // Click Type 과 Click Name 을 한 변수에 저장 2017-04-13
  s.prop55=s.pageName;
  s.eVar53="D=c53";   // eVar53 에 prop53 복사 2017-04-13 
  s.tl(true,'o','click links');
}

