/* Plugin Config */
s.usePlugins = true;
function s_doPlugins(s) { 

	s.events=s.events?s.events:"";
	var pgEscaped = escape(s.pageName);
	pgEscaped = pgEscaped.replace(/(%u2014|%u00AD|%u2013|%u2011|%u2012|%u002D)/g, '-');
	s.pageName = unescape(pgEscaped);   
	
	if(document.location.href.indexOf("ref=")>-1){
		s.referrer=s.getQueryParam("ref");
	}else{
		s.referrer=document.referrer;
	}
	
	if(s.referrer && s.referrer.lastIndexOf("http", 0) === 0 && s.referrer.charAt(6) !== "/") {
	  var decodeCount = 0;
	  while(s.referrer.charAt(6) !== "/" && decodeCount < 3) {
		s.referrer = decodeURIComponent(s.referrer);
		decodeCount++;
	  }
	}
 
  /* removes referrer if root domain is present in domain name
  var rd=window.location.hostname,hnp=rd.split("."),
      r=s.referrer.indexOf('/',8)>-1?s.referrer.slice(0,s.referrer.indexOf('/',8)):s.referrer;
  if(hnp.length>1){var rdp=hnp.slice(hnp.length-2);rd=rdp?"."+rdp.join("."):rd;}
  if(r&&r.indexOf(rd)>-1){s.referrer="";}
  s._1_referrer=1;
	s.referrer=s.getValOnce(s.referrer,"nike_referrer",0);
	*/  // 2017-04-06 레퍼러 삭제 이슈 해결

	if(s.pageType==="errorPage"){s.pageName='D="404:"+g';}

	s.prop54=(s.referrer)?s.referrer:"unknown"; // document.referrer 수정 2017-04-06
	s.server=s.account.split(",")[0];
	
	s.prop5=s.getPreviousValue(s.pageName,"c5","");
	if(typeof s.prop5=="undefined"||s.prop5==""){s.prop5="entry";}
	if(s.prop5=="no value"){s.prop5="no page name defined";}
	s.eVar70="D=c5";
	
	s.prop6=s.getPreviousValue(s.prop17,"c6","");
	if(typeof s.prop6=="undefined"||s.prop6==""){s.prop6="entry";}
	if(s.prop6=="no value"){s.prop6="no page type defined";}
	s.eVar60="D=c6";

  s.prop7=(s.pageName)?s.pageName:"";
	s.prop9=s_code_version;
	
	var ppv = s.getPercentPageViewed();
	s.prop23= ppv[1] + '|' + ppv[2];
	if(!ppv[1]){s.prop23="entry";}
	
	s.prop24="D=User-Agent";
	s.prop26="D=g";
	
	s.prop37=s.getTimeParting("f","+9");
	if(s.prop37){s.eVar16="D=c37";}
	
	//s.prop51=s.screenOrient.getPrevPageValue("c51",">>");
	
	s.eVar2=s.getQueryParam("et_rid");

	s.eVar5=s.getValOnce(decodeURI(s.getQueryParam("intpromo")),"v5");
	if(s.eVar5){s.events=s.apl(s.events,"event51",",",2);s.products=(s.products)?s.products:";";}

// nav 파라메터를 eVar6 에 태깅 및 event52 설정
	s.eVar6=s.getValOnce(s.getQueryParam("nav"),"v6");
	if(s.eVar6){s.events=s.apl(s.events,"event52",",",2);}

	var cmp=s.getQueryParam("cp,promoid"),etcmp=s.getQueryParam("et_cid");
	s.eVar7=s.getValOnce(cmp,"v7");
	if(!cmp&&etcmp){s.eVar7=s.getValOnce(etcmp,"v7");}
	
	if(s.prop15){s.eVar8="D=c15";}
	
	if(s.purchaseID){s.eVar10="D=purchaseID";}
	
	s.successfulSearchEvent="event2";
	s.nullSearchEvent="event37";
	s.searchTermVariable="prop11";
  var a;
	if(s.prop11){
		// 2016.11.22 재 검색 시 s.prop11을 잃어 버림
		//s.prop11=s.getValOnce(s.prop11,"c11",0);
		if(!s.prop11){
			a=s.split(s.events,","); var e="";
			for (var i=0;i<a.length;i+=1){
				if (a[i]!==s.successfulSearchEvent&&a[i]!==s.nullSearchEvent){
					e+=a[i]?a[i]+",":a[i];
				}
			}
			s.events=(e)?e.substring(0,e.length-1):"";
		} else {
			s.eVar11="D=c11";
			//s.products=(s.products)?s.products:";";
			s.eVar40="+1";
		}
	}
	if((s.events+",").indexOf("event2,")===-1){
		if(s.events.indexOf("event37")===-1){
			s.eVar71="";
		}
	}
	
	if(s.prop1){s.eVar12="D=c1";}
	if(s.prop1&&!s.products){s.products=";";}
	
	if(s.events&&s.events.indexOf("scAdd")>-1&&!s.c_r("v15")){s.ttc="start";}
	if(s.events&&s.events.indexOf("purchase")>-1){s.ttc="stop";}
	s.eVar15=s.getTimeToComplete(s.ttc,"v15",28);

	if(s.prop14){s.eVar23="D=c14";}

	if(s.prop2){s.eVar24="D=c2";}

	// Get the Monetate mt.m cookie value. May come from Tealium's Monetate SiteCatalyst Integration Preloader extension or from the cookie.
  if (typeof($) !== "undefined" && typeof($.parseJSON) !== "undefined") {
	  var mtm_cookie=$.parseJSON(decodeURIComponent(s.c_r('mt.m')) == "" ? "{}" : decodeURIComponent(s.c_r('mt.m')));
	  var mtm_cookie_membership=(mtm_cookie&&mtm_cookie.membership)?mtm_cookie.membership.join('|'):"";
	  s.eVar22=(window.utag_data && utag_data.monetate_membership) ? utag_data.monetate_membership : mtm_cookie_membership;
  }

	// Set the user guid from the nike.dat.guids library in Tealium
  if (typeof(nike) !== "undefined" && typeof(nike.dat) !== "undefined" && typeof(nike.dat.guids) !== "undefined" && typeof(nike.dat.guids.checkGUID) !== "undefined") {s.prop30=nike.dat.guids.checkGUID(nike.dat.guids.configU)||"";}

	if(s.events&&(s.events+",").indexOf("event4,")>-1){s.eVar33="+1";}
	
	s.eVar34="+1";
	
	if(s.prop36){s.eVar36="D=c36";}
	
	s.eVar42=s.getValOnce(s.getQueryParam("et_llid"),"v42");
	
	if(s.events&&s.events.indexOf("purchase")>-1){s.eVar46="+1";}
	
	s.eVar48="D=pageName";
	
	s.eVar50=s.getValOnce(s.getQueryParam("et_cid"),"v50");
	
	if(s.prop13){s.eVar62="D=c13";}
	
	if(s.prop12&&!s.products){s.products=";";}
	
	if(s.events&&s.events.indexOf("event20")>-1){s.eVar67="+1";}
	
	if(typeof $!=='undefined'){$('a[href*="intpromo="]').each(function(){s.list1=s.apl(s.list1,decodeURI(s.getQueryParam("intpromo",$(this).attr('href'))),"|",2)});}
	
  if(s.list1){s.events=s.apl(s.events,"event50",",",2);}
	
	//로그인 후 첫화면 event5 midus2864 
	if ($("#memberIdForGen").val() != null && $("#memberIdForGen").val() != undefined && $("#memberIdForGen").val() != '' && $("#memberIdForGen").val() != 'guest') {
		var referrerUrl = document.referrer;
		if(referrerUrl != null && referrerUrl != 'undefined' && referrerUrl != undefined && referrerUrl.indexOf("/gate.lecs") > -1){
			s.events=s.apl(s.events,"event5",",",2);
		}
	}
	
	// Set s.campaign variable for marketing channel
	if(!s.campaign) s.campaign=s.getQueryParam('cp');
	s.campaign=s.getValOnce(s.campaign,'s_campaign');
	
	for (a = 1; a <= 75; a++) {
		var propKey = "prop" + a;
		var eVarKey = "eVar" + a;
		var listKey = "list" + a;
		var propValue = s[propKey];
		var eVarValue = s[eVarKey];
		var listValue = s[listKey];
 
		if(propValue){
			s[propKey] = propValue.toLowerCase().replace(/^d=/, "D=");
		}
		if(eVarValue){
			s[eVarKey] = eVarValue.toLowerCase().replace(/^d=/, "D=");
		}
		if(a <= 3 && listValue){
			s[listKey] = listValue.toLowerCase().replace(/^d=/, "D=");
		}
	}
  var b = ["pageName","channel","server"];
  for (a=0; a < b.length; a++) {
		s[b[a]] && (s[b[a]] = s[b[a]].toLowerCase().replace(/^d=/, "D="));
	}
  
  /* Performance Timing */
  if(s.prop5!="entry"){
	  s.performanceTiming(); //list variable : optional	
  }
  
	s.plugins="";
}
s.doPlugins=s_doPlugins;

