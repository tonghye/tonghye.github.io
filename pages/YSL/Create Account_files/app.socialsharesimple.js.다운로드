(function(b){var c={facebook:{height:250,width:500},twitter:{height:280,width:500},google_plus:{height:550,width:500},pinterest:{height:580,width:780},tumblr:{height:300,width:500},reddit:{height:700,width:900},linkedin:{height:300,width:500},email:{height:300,width:500}};b(document).on("click",".social-share-buttons a.new-window",function(a){a.preventDefault();a.stopImmediatePropagation();a=b(this).attr("data-provider");var d=b(this).attr("href"),e=c[a]&&c[a].height||"300",f=c[a]&&c[a].width||"500",
g=b(window).height()/2-e/2,h=b(window).width()/2-f/2,d=window.open(d,a,"height\x3d"+e+", width\x3d"+f+", scrollbars\x3dyes, resizable\x3dyes, top\x3d"+g+", left\x3d"+h);b(document).trigger("simple.social.tracking",{provider:a});d&&window.focus&&d.focus()})})(jQuery);